	<queryString>
		<![CDATA[SELECT
  (SELECT description FROM m_setting_app WHERE id='ebtos.cabang.kota'
  ) kota,
  (SELECT description FROM m_setting_app WHERE id='ebtos.nama.cabang'
  ) AS cabang,
  r.no_reg,
  m.name,
  db.name AS debit,
  r.no_ppkb,
  mv.name AS vessel,
  pp.voy_in,
  pp.voy_out,
  p.discharge_service_type,
  r.status_service,
  db.address,
  r.pengurus_do,
  mc.language,
  mc.country,
  mc.default_fraction_digits,
  i.no_invoice,
  i.ppn,
  i.materai,
  i.created_date,
  COUNT(*) AS container_counts
FROM (((registration r
JOIN m_customer db
ON (r.cust_code = db.cust_code))
JOIN (planning_vessel p
JOIN (preservice_vessel pp
JOIN m_customer m
ON (pp.cust_code = m.cust_code)
JOIN m_vessel mv
ON (pp.vessel_code = mv.vessel_code))
ON (p.booking_code = pp.booking_code)
JOIN m_currency mc
ON (p.tipe_pelayaran = mc.trade_type))
ON (r.no_ppkb        = p.no_ppkb))
JOIN (uc_receiving_service urs
JOIN uncontainerized_service us
ON (urs.id_uc  = us.id_uc))
ON (urs.no_reg = r.no_reg))
JOIN invoice i
ON (r.no_reg   = i.no_reg)
WHERE r.no_reg = $P{no_reg}
GROUP BY r.no_reg,
  db.name,
  m.name,
  r.no_ppkb,
  mv.name,
  pp.voy_in,
  pp.voy_out,
  p.discharge_service_type,
  r.status_service,
  db.address,
  r.pengurus_do,
  mc.language,
  mc.country,
  mc.default_fraction_digits,
  i.no_invoice,
  i.ppn,
  i. materai,
  i.created_date]]>
	</queryString>
