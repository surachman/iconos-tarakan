	<queryString>
		<![CDATA[
SELECT rs.job_slip,
  rs.no_reg,
  r.no_ppkb,
  mv.name AS vessel,
  ct.iso_code,
  pp.voy_in,
  pp.voy_out,
  p.eta,
  rs.cont_no,
  rs.cont_size,
  rs.cont_status,
  rs.cont_gross,
  rs.cont_type AS type,
  rs.valid_date ,
  i.no_invoice,
  rs.bl_no,
  ms.name AS cust_name,
  i.payment_date,
  rs.over_size,
  scd.disch_port,
  ma.description,
  rs.commodity
FROM stripping_service rs
JOIN m_container_type ct
ON (rs.cont_type = ct.cont_type)
JOIN registration r
ON (r.no_reg = rs.no_reg)
JOIN planning_vessel p
ON (r.no_ppkb = p.no_ppkb)
JOIN preservice_vessel pp
ON (p.booking_code = pp.booking_code)
JOIN m_vessel mv
ON (pp.vessel_code = mv.vessel_code)
JOIN invoice i
ON (r.no_reg=i.no_reg)
JOIN m_customer ms
ON (r.cust_code=ms.cust_code)
JOIN service_cont_discharge scd
ON (rs.cont_no = scd.cont_no
AND r.no_ppkb  = scd.no_ppkb)
JOIN perhitungan_stripping ps
ON (rs.perhitungan_stripping_id = ps.id)
LEFT JOIN perhitungan_sewa_alat psa
ON (ps.perhitungan_sewa_alat_id = psa.id)
LEFT JOIN m_activity ma
ON (psa.activity_code = ma.activity_code)
WHERE rs.job_slip     = $P{job_slip}
]]>
	</queryString>
