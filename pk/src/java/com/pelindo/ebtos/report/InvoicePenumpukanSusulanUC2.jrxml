	<queryString>
		<![CDATA[
SELECT
  (SELECT description || ', ' ||
    (SELECT description FROM m_setting_app WHERE id IN ('ebtos.cabang.kota')
    )
  FROM m_setting_app
  WHERE id IN ('ebtos.cabang.alamat')
  ) alamat,
  (SELECT description FROM m_setting_app WHERE id='ebtos.cabang.kota'
  ) kota,
  (SELECT description FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) description,
  (SELECT value_string FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) nama_ttd,
  (SELECT value_nip FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) nip_ttd,
  z.language,
  z.country,
  z.default_fraction_digits,
  c.name,
  (SELECT cus.name FROM m_customer cus WHERE cus.cust_code = r.cust_code
  ) AS debit,
  (SELECT cus.npwp FROM m_customer cus WHERE cus.cust_code = r.cust_code
  ) AS npwp,
  (SELECT cus.address FROM m_customer cus WHERE cus.cust_code = r.cust_code
  ) AS address,
  (SELECT value_string FROM m_setting_app WHERE id = 'ebtos.company.npwp'
  ) npwp_company,
  (SELECT value_string
  FROM m_setting_app
  WHERE id = 'ebtos.company.ppkp'
  ) ppkp_company,
  changedate(TO_CHAR(
  (SELECT value_date FROM m_setting_app WHERE id = 'ebtos.company.ppkp_tmt'
  ), 'dd/mm/yyyy')) AS ppkp_tmt_company,
  r.no_ppkb,
  mv.name AS vessel,
  pp.voy_in,
  pp.voy_out,
  r.no_reg,
  RPAD(s.service_name,42, ' SERVICE') AS service_name,
  i.payment_type,
  i.no_invoice,
  i.jumlah_tagihan,
  i.materai,
  i.ppn,
  i.total_tagihan,
  tb1.totalTumpuk,
  z.symbol,
  TO_CHAR(i.modified_date, 'dd/MM/yyyy')   AS tanggal,
  changedate(TO_CHAR(now(), 'dd/mm/yyyy')) AS tanggal2,
  TO_CHAR(now(), 'yyyymm.')                AS tanggal3,
  map.description                          AS nmttd,
  map.value_string                         AS nipttd,
  TO_CHAR(i.modified_date,'dd/MM/yyyy') modified_date,
  i.modified_by,
  i.created_date,
  i.payment_status,
  i.payment_tool,
  r.pengurus_do,
  i.no_faktur_pajak,
  (SELECT COUNT(ds.job_slip)
  FROM uc_penumpukan_susulan_service ds,
    perhitungan_penumpukan_susulan pp,
    m_currency c,
    uncontainerized_service us,
    m_commodity co
  WHERE co.commodity_code = us.commodity
  AND ds.id_uc            = us.id_uc
  AND pp.bl_no            = us.bl_no
  AND pp.curr_code        =c.curr_code
  AND pp.no_reg           = ds.no_reg
  AND ds.no_reg           = $P{no_reg}
  ) AS totBox,
  seq_print(r.no_reg) invstatus
FROM m_currency z,
  m_service s,
  m_customer c,
  registration r,
  planning_vessel pv,
  preservice_vessel pp,
  m_vessel mv,
  invoice i,
  m_setting_app map,
  (SELECT NVLN(SUM(total_charge)) AS totalTumpuk
  FROM perhitungan_penumpukan_susulan
  WHERE no_reg = $P{no_reg}
  ) tb1
WHERE r.service_code = s.service_code
AND i.no_reg         = r.no_reg
AND r.no_ppkb        = pv.no_ppkb
AND pv.booking_code  = pp.booking_code
AND pp.vessel_code   = mv.vessel_code
AND pp.cust_code     = c.cust_code
AND r.no_reg         = $P{no_reg}
AND i.curr_code      =z.curr_code
AND map.id           ='ebtos.ttd.invoice'
]]>
	</queryString>
