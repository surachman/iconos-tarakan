	<queryString>
		<![CDATA[SELECT
  (SELECT description ||', ' ||
    (SELECT description FROM m_setting_app WHERE id IN ('ebtos.cabang.kota')
    )
  FROM m_setting_app
  WHERE id IN ('ebtos.cabang.alamat')
  ) alamat,
  (SELECT description FROM m_setting_app WHERE id='ebtos.cabang.kota'
  ) kota,
  (SELECT description FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) description,
  (SELECT value_string FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) nama_ttd,
  (SELECT value_nip FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) nip_ttd,
  sv.no_ppkb,
  sv.discharge_service_type,
  prv.voy_in,
  prv.voy_out,
  mv.name vessel_name,
  mc.name customer_name,
  mc.address,
  mc.npwp,
  r.no_reg,
  i.no_invoice,
  i.created_date invoice_date,
  i.no_faktur_pajak,
  (NVLN(rh.handling_charge) + NVLN(rhu.handling_charge) + NVLN(rop.total_charge)) handling_charge,
  (NVLN(rp.total_charge)    + NVLN(rpu.total_charge)) penumpukan_charge,
  NVLN(rhm.total_charge) hatchmoves_charge,
  (NVLN(rh.transhipment_charge) + NVLN(rhu.transhipment_charge)) transhipment_charge,
  (NVLN(rh.shifting_charge)     + NVLN(rhu.shifting_charge)) shifting_charge,
  NVLN(rak.total_charge) air_kapal_charge,
  i.additional_charge upah_buruh_charge,
  ulem.total_charge upah_lembur,
  i.total_tagihan,
  i.jumlah_tagihan,
  (SELECT value_string FROM m_setting_app WHERE id = 'ebtos.company.npwp'
  ) npwp_company,
  (SELECT value_string
  FROM m_setting_app
  WHERE id = 'ebtos.company.ppkp'
  ) ppkp_company,
  changedate(TO_CHAR(
  (SELECT value_date FROM m_setting_app WHERE id = 'ebtos.company.ppkp_tmt'
  ), 'dd/mm/yyyy')) AS ppkp_tmt_company,
  i.ppn,
  i.materai,
  mgr.description nama_mgr,
  mgr.value_string nip_mgr,
  ws.value_string workstation,
  ms.service_name,
  mcr.language,
  mcr.country,
  mcr.default_fraction_digits,
  (EXTRACT (DAY FROM i.created_date)
  || ' '
  || bulan(i.created_date)
  || ' '
  || EXTRACT (YEAR FROM i.created_date)) formatted_invoice_date
FROM ((service_vessel sv
JOIN (planning_vessel pv
JOIN m_vessel mv
ON (pv.vessel_code=mv.vessel_code))
ON (sv.no_ppkb    =pv.no_ppkb))
JOIN (preservice_vessel prv
JOIN m_customer mc
ON (prv.cust_code  =mc.cust_code))
ON (pv.booking_code=prv.booking_code))
JOIN
  (
  SELECT $P{no_ppkb} no_ppkb, $P{curr_code} curr_code
  FROM DUAL
  ) q
ON (q.no_ppkb=sv.no_ppkb)
LEFT JOIN
  (SELECT no_ppkb,
    curr_code,
    SUM(
    CASE
      WHEN activity = 'DISCHARGE'
      THEN total_charge
      ELSE 0
    END) handling_charge,
    SUM(
    CASE
      WHEN activity = 'TRANSHIPMENT'
      THEN total_charge
      ELSE 0
    END) transhipment_charge,
    SUM(
    CASE
      WHEN activity = 'SHIFTING'
      THEN total_charge
      ELSE 0
    END) shifting_charge
  FROM vw_perhitungan_handling_petike
  WHERE operation = 'DISCHARGE'
  GROUP BY no_ppkb,
    curr_code
  ) rh
ON (q.no_ppkb  =rh.no_ppkb
AND q.curr_code=rh.curr_code)
LEFT JOIN
  (SELECT no_ppkb,
    curr_code,
    SUM(total_charge) total_charge
  FROM vw_perhitungan_penumpukan_bong
  GROUP BY no_ppkb,
    curr_code
  ) rp
ON (q.no_ppkb  =rp.no_ppkb
AND q.curr_code=rp.curr_code)
LEFT JOIN
  (SELECT no_ppkb,
    curr_code,
    SUM(total_charge) total_charge
  FROM recap_penumpukan_uc
  WHERE operation = 'DISCHARGE'
  GROUP BY no_ppkb,
    curr_code
  ) rpu
ON (q.no_ppkb  =rpu.no_ppkb
AND q.curr_code=rpu.curr_code)
LEFT JOIN
  (SELECT no_ppkb,
    curr_code,
    SUM(
    CASE
      WHEN activity = 'SHIFTING'
      THEN total_charge
      ELSE 0
    END) shifting_charge,
    SUM(
    CASE
      WHEN activity = 'TRANSHIPMENT'
      THEN total_charge
      ELSE 0
    END) transhipment_charge,
    SUM(
    CASE
      WHEN activity = 'LOAD'
      OR activity   = 'DISCHARGE'
      THEN total_charge
      ELSE 0
    END) handling_charge
  FROM recap_handling_uc
  WHERE operation = 'DISCHARGE'
  GROUP BY no_ppkb,
    curr_code
  ) rhu
ON (q.no_ppkb  =rhu.no_ppkb
AND q.curr_code=rhu.curr_code)
LEFT JOIN
  (SELECT no_ppkb,
    curr_code,
    SUM(total_charge) total_charge
  FROM recap_hatch_move WHERE operation='DISCHARGE'
  GROUP BY no_ppkb,
    curr_code
  ) rhm
ON (q.no_ppkb  =rhm.no_ppkb
AND q.curr_code=rhm.curr_code)
LEFT JOIN
  (SELECT no_ppkb,
    curr_code,
    SUM(total_charge) total_charge
  FROM recap_upah_buruh
  WHERE operation = 'DISCHARGE'
  GROUP BY no_ppkb,
    curr_code
  ) rub
ON (q.no_ppkb  =rub.no_ppkb
AND q.curr_code=rub.curr_code)
LEFT JOIN
  (SELECT no_ppkb,
    curr_code,
    SUM(total_charge) total_charge
  FROM recap_upah_buruh_uc
  WHERE operation = 'DISCHARGE'
  GROUP BY no_ppkb,
    curr_code
  ) rubu
ON (q.no_ppkb  =rubu.no_ppkb
AND q.curr_code=rubu.curr_code)
LEFT JOIN
  (SELECT no_ppkb,
    curr_code,
    SUM(total_charge) total_charge
  FROM recap_air_kapal
  GROUP BY no_ppkb,
    curr_code
  ) rak
ON (q.no_ppkb  =rak.no_ppkb
AND q.curr_code=rak.curr_code)
LEFT JOIN
  (SELECT no_ppkb,
    curr_code,
    SUM(total_charge) total_charge
  FROM recap_other_package WHERE main_activity='DISCHARGE'
  GROUP BY no_ppkb,
    curr_code
  ) rop
ON (q.no_ppkb  =rop.no_ppkb
AND q.curr_code=rop.curr_code)
LEFT JOIN
  (SELECT D.no_ppkb, A.curr_code, A.TOTAL_TAGIHAN total_charge
FROM INVOICE A INNER JOIN PERHITUNGAN_NOTA_MANUAL B ON A.NO_REG=B.NO_REG
INNER JOIN (SELECT DISTINCT NO_REG FROM PERHITUNGAN_NOTA_MANUAL_DETAIL WHERE ACTIVITY_TYPE=6) C ON B.NO_REG=C.NO_REG
INNER JOIN NO_PERHITUNGAN_BENTUK_TIGA D ON B.NO_BENTUK_TIGA=D.NO_REGISTRASI
) ulem
ON (ulem.no_ppkb=q.no_ppkb AND ulem.curr_code=q.curr_code)
JOIN ((registration r
JOIN (invoice i
JOIN m_currency mcr
ON (i.curr_code=mcr.curr_code))
ON (r.no_reg   =i.no_reg))
JOIN m_service ms
ON (r.service_code=ms.service_code))
ON (sv.no_ppkb    =r.no_ppkb),
  (SELECT description,
    value_string
  FROM m_setting_app
  WHERE id='ebtos.ttd.invoice'
  ) mgr,
  (SELECT value_string FROM m_setting_app WHERE id='ebtos.workstation'
  ) ws
WHERE r.service_code = 'SCHDD'
AND i.curr_code      = q.curr_code]]>
	</queryString>
