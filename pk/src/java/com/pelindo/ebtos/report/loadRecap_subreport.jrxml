	<queryString>
		<![CDATA[
SELECT s.cont_no,
  s.cont_status,
  s.cont_size,
  c.type_in_general AS name,
  s.transaction_date,
  e.start_time,
  r.masa_1,
  r.masa_2,
  NVLN(p.charge_m1)    AS charge_m1,
  NVLN(p.charge_m2)    AS charge_m2,
  NVLN(p.jasa_dermaga) AS jasa_dermaga,
  NVLN(p.total_charge) AS total_charge,
  z.symbol
FROM (((( service_receiving s
LEFT JOIN m_container_type c
ON s.cont_type=c.cont_type)
LEFT JOIN equipment e
ON s.cont_no            =e.cont_no
AND s.no_ppkb           =e.no_ppkb
AND e.operation         ='LOAD'
AND e.equipment_act_code='LIFTON')
LEFT JOIN receiving_service r
ON s.cont_no =r.cont_no
AND s.no_ppkb=r.no_ppkb)
LEFT JOIN perhitungan_penumpukan p
ON s.cont_no =p.cont_no
AND s.no_ppkb=p.no_ppkb),
  m_currency z
WHERE p.curr_code=z.curr_code
AND s.no_ppkb    =$P{no_ppkb}
]]>
	</queryString>
