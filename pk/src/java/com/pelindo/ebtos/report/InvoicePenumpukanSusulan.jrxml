	<queryString>
		<![CDATA[
SELECT
  (SELECT description || ', ' ||
    (SELECT description FROM m_setting_app WHERE id IN ('ebtos.cabang.kota')
    )
  FROM m_setting_app
  WHERE id IN ('ebtos.cabang.alamat')
  ) alamat,
  (SELECT description FROM m_setting_app WHERE id='ebtos.cabang.kota'
  ) kota,
  (SELECT description FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) description,
  (SELECT value_string FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) nama_ttd,
  (SELECT value_nip FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) nip_ttd,
  z.language,
  z.country,
  z.default_fraction_digits,
  dbt.name debit,
  dbt.npwp,
  dbt.address,
  c.name pelayaran,
  r.no_ppkb,
  mv.name AS vessel,
  r.no_reg,
  RPAD(s.service_name,27, ' SERVICE') AS service_name,
  i.payment_type,
  i.no_invoice,
  i.jumlah_tagihan,
  i.materai,
  i.ppn,
  i.total_tagihan,
  tb1.totalTumpuk,
  tb1.masa1,
  tb1.masa2,
  z.symbol,
  pp.voy_in,
  pp.voy_out,
  TO_CHAR(now(),'dd/MM/yyyy')              AS tanggal,
  changedate(TO_CHAR(now(), 'dd/mm/yyyy')) AS tanggal2,
  TO_CHAR(now(), 'yyyymm.')                AS tanggal3,
  map.description                          AS nmttd,
  map.value_string                         AS nipttd,
  TO_CHAR(i.modified_date,'dd/MM/yyyy') modified_date,
  i.modified_by,
  i.created_date,
  i.payment_status,
  i.payment_tool,
  r.pengurus_do,
  i.no_faktur_pajak,
  (SELECT value_string FROM m_setting_app WHERE id = 'ebtos.company.npwp'
  ) npwp_company,
  (SELECT value_string
  FROM m_setting_app
  WHERE id = 'ebtos.company.ppkp'
  ) ppkp_company,
  changedate(TO_CHAR(
  (SELECT value_date FROM m_setting_app WHERE id = 'ebtos.company.ppkp_tmt'
  ), 'dd/mm/yyyy')) AS ppkp_tmt_company,
  (SELECT COUNT(ds.job_slip) AS contoto
  FROM penumpukan_susulan_service ds,
    perhitungan_penumpukan_susulan pp
  WHERE pp.cont_no = ds.cont_no
  AND pp.no_reg    = ds.no_reg
  AND ds.no_reg    = $P{no_reg}
  ) AS totBox,
  (SELECT COUNT(ds.job_slip) AS contoto
  FROM penumpukan_susulan_service ds,
    perhitungan_penumpukan_susulan pp
  WHERE pp.cont_no = ds.cont_no
  AND pp.no_reg    = ds.no_reg
  AND ds.no_reg    = $P{no_reg}
  AND ds.cont_size =20
  ) AS totBox20,
  (SELECT COUNT(ds.job_slip) AS contoto
  FROM penumpukan_susulan_service ds,
    perhitungan_penumpukan_susulan pp
  WHERE pp.cont_no = ds.cont_no
  AND pp.no_reg    = ds.no_reg
  AND ds.no_reg    = $P{no_reg}
  AND ds.cont_size =40
  ) AS totBox40,
  (SELECT placement_date
  FROM
    (SELECT DISTINCT(TO_CHAR(pn.placement_date,'dd/MM/yyyy')) AS placement_date,
      pn.created_date
    FROM penumpukan_susulan_service pn
    WHERE pn.no_reg = $P{no_reg}
    AND ROWNUM < 2
    ORDER BY pn.created_date DESC
    ) T
  ) placement_date,
  (SELECT DISTINCT(TO_CHAR(pn.created_date,'dd/MM/yyyy')) AS placement_date
  FROM penumpukan_susulan_service pn
  WHERE pn.no_reg = $P{no_reg}
  ) created_date ,
  seq_print(r.no_reg) invstatus
FROM m_currency z,
  m_service s,
  m_customer c,
  registration r
JOIN m_customer dbt
ON (r.cust_code = dbt.cust_code),
  planning_vessel pv,
  preservice_vessel pp,
  m_vessel mv,
  invoice i,
  m_setting_app map,
  (SELECT NVLN(SUM(total_charge)) AS totalTumpuk,
    NVLN(SUM(charge_m1))          AS masa1,
    NVLN(SUM(charge_m2))          AS masa2
  FROM perhitungan_penumpukan_susulan
  WHERE no_reg = $P{no_reg}
  ) tb1
WHERE r.service_code = s.service_code
AND i.no_reg         = r.no_reg
AND r.no_ppkb        = pv.no_ppkb
AND pv.booking_code  = pp.booking_code
AND pp.vessel_code   = mv.vessel_code
AND pp.cust_code     = c.cust_code
AND r.no_reg         = $P{no_reg}
AND i.curr_code      =z.curr_code
AND map.id           ='ebtos.ttd.invoice']]>
	</queryString>
