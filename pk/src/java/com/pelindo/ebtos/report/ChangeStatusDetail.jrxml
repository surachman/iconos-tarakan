(select description from m_setting_app where id='ebtos.nama.cabang') as cabang,
(select cus.name from m_customer cus where cus.cust_code = pp.cust_code) as debit,
m.name, r.no_ppkb, mv.name as vessel, pp.voy_in, pp.voy_out, r.status_service,m.address,to_char(now(),
'dd/MM/yyyy HH:mm') as tanggal,
r.pengurus_do,
(SELECT count(ds.job_slip) as totcont FROM change_status_service ds
WHERE ds.no_reg = $P{no_reg}) totcont

FROM registration r, m_customer m, planning_vessel p, preservice_vessel pp, m_vessel mv
WHERE r.no_ppkb = p.no_ppkb AND r.cust_code = m.cust_code AND p.booking_code = pp.booking_code
AND pp.vessel_code = mv.vessel_code AND r.no_reg = $P{no_reg}]]>
