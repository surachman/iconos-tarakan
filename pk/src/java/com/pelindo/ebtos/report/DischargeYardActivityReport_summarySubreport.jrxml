	<parameter name="no_ppkb" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT cnt.cont_size,
  mct.type_in_general,
  cnt.cont_status,
  cnt.over_size,
  cnt.is_seling,
  cnt.dangerous,
  cnt.twenty_one_feet,
  cnt.is_import,
  COUNT(*)
FROM
  (SELECT scd.no_ppkb,
    scd.cont_no,
    scd.cont_size,
    scd.cont_type,
    scd.cont_status,
    scd.cont_gross,
    scd.over_size,
    scd.is_seling,
    scd.dangerous,
    scd.twenty_one_feet,
    scd.is_import,
    scd.block,
    scd.y_slot,
    scd.y_row,
    scd.y_tier,
    e.equip_code
  FROM service_cont_discharge scd
  JOIN equipment e
  ON (scd.cont_no          =e.cont_no
  AND scd.no_ppkb          =e.no_ppkb
  AND e.operation          = 'DISCHARGE'
  AND e.equipment_act_code = 'LIFTOFF')
  UNION ALL
  SELECT sct.no_ppkb,
    sct.cont_no,
    sct.cont_size,
    sct.cont_type,
    sct.cont_status,
    sct.cont_gross,
    sct.over_size,
    sct.is_sling,
    sct.dangerous,
    sct.twenty_one_feet,
    sct.is_export_import,
    sct.block,
    sct.y_slot,
    sct.y_row,
    sct.y_tier,
    e.equip_code
  FROM service_cont_transhipment sct
  JOIN equipment e
  ON (sct.cont_no          =e.cont_no
  AND sct.no_ppkb          =e.no_ppkb
  AND e.operation          = 'TRANSDISCHARGE'
  AND e.equipment_act_code = 'LIFTOFF')
  UNION ALL
  SELECT ss.no_ppkb,
    ss.cont_no,
    ss.cont_size,
    ss.cont_type,
    ss.cont_status,
    ss.cont_gross,
    ss.over_size,
    ss.is_sling,
    ss.dg,
    ss.twenty_one_feet,
    ss.is_export_import,
    psd.block,
    psd.y_slot,
    psd.y_row,
    psd.y_tier,
    e.equip_code
  FROM service_shifting ss
  JOIN planning_shift_discharge psd
  ON (ss.cont_no=psd.cont_no
  AND ss.no_ppkb=psd.no_ppkb)
  JOIN equipment e
  ON (ss.cont_no           =e.cont_no
  AND ss.no_ppkb           =e.no_ppkb
  AND e.operation          = 'SHIFTING-CY'
  AND e.equipment_act_code = 'LIFTOFF')
  ) cnt
JOIN m_container_type mct
ON (cnt.cont_type =mct.cont_type)
WHERE cnt.no_ppkb = $P{no_ppkb}
GROUP BY cnt.cont_size,
  mct.type_in_general,
  cnt.cont_status,
  cnt.over_size,
  cnt.is_seling,
  cnt.dangerous,
  cnt.twenty_one_feet,
  cnt.is_import]]>
