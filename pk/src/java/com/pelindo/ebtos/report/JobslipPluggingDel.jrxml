SELECT rs.job_slip,
  rs.no_reg,
  rs.cont_no,
  ct.iso_code,
  rs.cont_size,
  rs.cont_status,
  rs.cont_gross,
  ct.name AS type ,
  rs.bl_no ,
  rs.bl_no,
  ms.name AS cust_name
FROM registration r,
  plugging_reefer_service rs,
  m_container_type ct ,
  m_customer ms
WHERE rs.cont_type = ct.cont_type
AND r.no_reg       = rs.no_reg_del
AND r.cust_code    = ms.cust_code
AND r.no_reg       = $P{no_reg}
AND rs.job_slip    = $P{job_slip}]]>
