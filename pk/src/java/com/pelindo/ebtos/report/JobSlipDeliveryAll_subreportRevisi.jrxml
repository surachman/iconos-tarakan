	<queryString>
		<![CDATA[
SELECT rs.job_slip,
  rs.no_reg,
  rs.no_ppkb,
  mv.name AS vessel,
  pp.voy_in,
  pp.voy_out,
  p.eta,
  rs.cont_no,
  rs.cont_size,
  rs.cont_status,
  rs.cont_gross,
  ct.name AS type,
  rs.valid_date,
  rs.placement_date,
  i.no_invoice,
  rs.bl_no,
  ms.name AS cust_name,
  (SELECT to_char(COUNT(scd.id))
  FROM service_cont_discharge scd,
    registration r
  WHERE scd.no_ppkb=r.no_ppkb
  AND r.no_reg     = $P{no_reg}
  ) countContDisch,
  (SELECT COUNT(scd.job_slip)
  FROM delivery_service scd,
    registration r
  WHERE scd.no_ppkb=r.no_ppkb
  AND r.no_reg     = $P{no_reg}
  ) countdelivery,
  (SELECT COUNT(scd.job_slip)
  FROM delivery_service scd
  WHERE scd.no_reg = $P{no_reg}
  )contcountdelivery
FROM registration r,
  delivery_service rs,
  planning_vessel p,
  preservice_vessel pp,
  m_vessel mv,
  m_container_type ct,
  invoice i,
  m_customer ms
WHERE r.no_ppkb    = p.no_ppkb
AND p.booking_code = pp.booking_code
AND pp.vessel_code = mv.vessel_code
AND rs.cont_type   = ct.cont_type
AND r.no_reg       = rs.no_reg
AND r.no_reg       =i.no_reg
AND r.cust_code    =ms.cust_code
AND r.no_reg       = $P{no_reg}
]]>
	</queryString>
