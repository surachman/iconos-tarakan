	<queryString>
		<![CDATA[SELECT
  (SELECT description ||', ' ||
    (SELECT description FROM m_setting_app WHERE id IN ('ebtos.cabang.kota')
    )
  FROM m_setting_app
  WHERE id IN ('ebtos.cabang.alamat')
  ) alamat,
  (SELECT description FROM m_setting_app WHERE id='ebtos.cabang.kota'
  ) kota,
  (SELECT description FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) description,
  (SELECT value_string FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) nama_ttd,
  (SELECT value_nip FROM m_setting_app WHERE id='ttd_ebtos_mgr_keu'
  ) nip_ttd,
  r.no_reg,
  m.name,
  db.name AS debit,
  db.npwp,
  db.address,
  r.no_ppkb,
  mv.name AS vessel,
  pp.voy_in,
  pp.voy_out,
  p.discharge_service_type,
  r.status_service,
  m.address,
  r.pengurus_do,
  mc.language,
  mc.country,
  mc.default_fraction_digits,
  i.ppn,
  i. materai,
  i.no_invoice,
  i.no_faktur_pajak,
  i.created_date,
  i.jumlah_tagihan,
  i.total_tagihan,
  map.description                          AS nmttd,
  map.value_string                         AS nipttd,
  RPAD(ms.service_name, 32, ' SERVICE')    AS service_name,
  COUNT(*)                                 AS container_counts,
  changedate(TO_CHAR(now(), 'dd/mm/yyyy')) AS now,
  (SELECT value_string FROM m_setting_app WHERE id = 'ebtos.company.npwp'
  ) npwp_company,
  (SELECT value_string
  FROM m_setting_app
  WHERE id = 'ebtos.company.ppkp'
  ) ppkp_company,
  changedate(TO_CHAR(
  (SELECT value_date FROM m_setting_app WHERE id = 'ebtos.company.ppkp_tmt'
  ), 'dd/mm/yyyy')) AS ppkp_tmt_company,
  NVLN(SUM(ups.charge)) handling_charge,
  NVLN(SUM(ppg.total_charge)) pass_gate_charge,
  NVLN(SUM(upl.charge)) lift_charge,
  i.additional_charge,
  NVLN(SUM(upp.charge)) penumpukan_charge,
  NVLN(SUM(ups.jasa_dermaga)) jasa_dermaga_charge
FROM m_setting_app map,
  ((((registration r
JOIN m_customer db
ON (r.cust_code = db.cust_code))
JOIN (planning_vessel p
JOIN (preservice_vessel pp
JOIN m_customer m
ON (pp.cust_code = m.cust_code)
JOIN m_vessel mv
ON (pp.vessel_code = mv.vessel_code))
ON (p.booking_code = pp.booking_code))
ON (r.no_ppkb      = p.no_ppkb))
JOIN (uc_receiving_service urs
JOIN uncontainerized_service us
ON (urs.id_uc = us.id_uc)
LEFT JOIN uc_perhitungan_penumpukan upp
ON (upp.jobslip = urs.jobslip)
LEFT JOIN uc_perhitungan_lift upl
ON (upl.jobslip = urs.jobslip)
LEFT JOIN perhitungan_upah_buruh pub
ON (pub.jobslip=urs.jobslip)
LEFT JOIN uc_perhitungan_stevedoring ups
ON (ups.jobslip = urs.jobslip)
LEFT JOIN perhitungan_pass_gate ppg
ON (ppg.job_slip = urs.jobslip))
ON (urs.no_reg   = r.no_reg))
JOIN (invoice i
JOIN m_currency mc
ON (i.curr_code = mc.curr_code))
ON (r.no_reg    = i.no_reg))
JOIN m_service ms
ON (ms.service_code = r.service_code)
WHERE r.no_reg      = $P{no_reg}
AND map.id          ='ebtos.ttd.invoice'
GROUP BY r.no_reg,
  db.name,
  db.npwp,
  db.address,
  m.name,
  r.no_ppkb,
  mv.name,
  pp.voy_in,
  pp.voy_out,
  p.discharge_service_type,
  r.status_service,
  m.address,
  r.pengurus_do,
  mc.language,
  mc.country,
  mc.default_fraction_digits,
  i.ppn,
  i. materai,
  i.no_invoice,
  i.no_faktur_pajak,
  ms.service_name,
  i.jumlah_tagihan,
  i.total_tagihan,
  i.created_date,
  map.description,
  map.value_string,
  i.additional_charge]]>
	</queryString>
