<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
    version="2.0">
    <namespace>http://www.qtasnim.com/currency</namespace>

    <function>
        <function-name>getFormattedCurrency</function-name>
        <function-class>com.qtasnim.el.QTasnim</function-class>
        <function-signature>java.lang.String getFormattedCurrency(java.lang.String, java.lang.String, java.lang.Object, java.lang.Boolean, java.lang.Integer)</function-signature>
    </function>

    <function>
        <function-name>getCurrencySymbol</function-name>
        <function-class>com.qtasnim.el.QTasnim</function-class>
        <function-signature>java.lang.String getCurrencySymbol(java.lang.String, java.lang.String)</function-signature>
    </function>
</facelet-taglib>